import random
length = [5,6,7,8,9]
lengthlist = random.choices(length, k=1000)
print(lengthlist)
letters = ['a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z']
def namgen(x):
  names = random.choices(letters, k=x)
  print(''.join(names))
def name1000():
  for char in lengthlist:
    namgen(char)
name1000()
